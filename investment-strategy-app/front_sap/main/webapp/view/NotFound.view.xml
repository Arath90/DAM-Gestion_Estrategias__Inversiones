<mvc:View controllerName="main.controller.NotFound"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.f"
    xmlns:m="sap.m">
    <DynamicPage toggleHeaderOnTitleClick="false">
        <title>
            <DynamicPageTitle><heading><m:Title text="Contenido no encontrado" level="H2" /></heading></DynamicPageTitle>
        </title>
        <content>
            <m:VBox class="sapUiLargeMargin">
                <m:MessageStrip type="Warning" text="La ruta solicitada no existe. Seras redirigido al inicio." showIcon="true" />
            </m:VBox>
        </content>
    </DynamicPage>
</mvc:View>


